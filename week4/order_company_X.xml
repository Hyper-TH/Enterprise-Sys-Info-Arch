<?xml version="1.0"?>
<?xml-stylesheet href="order_transform_X_Y.xsl" type="text/xsl"?>

<!-- Root element is now "order_document" -->
<order orderid="889923" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="order_company_X.xsd">

	<!-- Person element is now "person_responsible-->
	<person>John Smith</person>
	
	<!-- New sub root element "parties" -->
	
	<!-- Shipto element is now "ship_to_party" -->
	<shipto>
		<name>Ola Nordmann</name>	<!-- Same -->
		
		<!-- New sub root element "address_info" -->
		<address>4000 Langgt 23</address> <!-- "address_ line" -->
		<city>Stavanger</city>
		<country>Norway</country>	<!-- "destination_country -->
	</shipto>
	<!-- End "ship_to_party" -->
	<!-- End "parties" -->
	<!-- End "person_responsible" -->
	
	<!-- Items are now in an element called "items" -->
	<!-- Items themselves are unchanged -->
	<item>
		<item-name>Item 1</item-name>
		<title>Empire Burlesque</title>
		<id>001</id>
		<note>Special Edition</note>
		<quantity>1</quantity>
		<price>10.90</price>
	</item>
  
	<item>
		<item-name>Item 2</item-name>
		<title>Hide your heart</title>
		<id>002</id>
		<quantity>2</quantity>
		<price>9.90</price>
	</item>
	  
</order>